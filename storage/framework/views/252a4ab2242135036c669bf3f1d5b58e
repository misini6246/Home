<script>
    $('.tixian_jilu_zt li').click(function () {
        $(this).addClass('active').siblings().removeClass('active');
        var index = $(this).index();
        $('.tixian_log').hide();
        if (index == 0) {
            $('.all_tixian').show();
        } else if (index == 1) {
            $('.jinxing_tixian').show();
        } else if (index == 2) {
            $('.wancheng_tixian').show();
        }
    })
</script>